      <netsuite.party.RoleType description="Executive" roleTypeId="Executive"/>
      <netsuite.party.RoleType description="Manager" roleTypeId="Manager"/>
      <netsuite.party.RoleType description="Operator" roleTypeId="Operator"/>
      <netsuite.party.RoleType description="Production Manager" roleTypeId="ProductionManager"/>
      <netsuite.party.RoleType description="Production Worker" roleTypeId="ProductionWorker"/>
      <netsuite.party.RoleType description="Receiver" roleTypeId="Receiver"/>
      <netsuite.party.RoleType description="Sales Contact" roleTypeId="Contact"/>
      <netsuite.party.RoleType description="Shipment Clerk" roleTypeId="ShipmentClerk"/>
      <netsuite.party.RoleType description="Supplier Agent" roleTypeId="SupplierAgent"/>
      <netsuite.party.RoleType description="Technical Contact" roleTypeId="TechContact"/>


      <moqui.basic.Enumeration description="Person Roles" enumId="RgpPerson" enumTypeId="RoleGroup"/>
